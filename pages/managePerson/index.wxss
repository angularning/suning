page {
  background: #f4f4f4;
}
.personList{
  background: #fff;
  padding: 4% 3%;
}
.personListRepeat{
  margin-bottom: 20rpx;
}
.personListName{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 32rpx;
  font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(51,51,51,1);
}
.personListTel{
  height: 50rpx;
  line-height: 50rpx;
font-size:24rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(204,204,204,1);
}
.personListBtn image{
  width:73px;
height:60rpx;
background:rgba(255,72,72,1);
margin-top:20rpx;
}
.personTip{
  position: absolute;
  bottom: 15%;
  font-size:12px;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(153,153,153,1);
text-align: center;
width: 100%;
}