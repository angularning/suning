/**start.wxss**/
page{
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  font-size: 10pt;
  line-height: 150%;
  min-height: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: stretch;
}
.container {
  position: relative;
  flex: 1;
  display: flex;
  flex-direction: column;
  background: #FFDE04;
  background-image: url('http://pbzxefl02.bkt.clouddn.com/sunLoginBg.png');
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: top center;
  align-items: stretch;
  padding: 0;
  height: 100%;
  overflow: hidden;
  justify-content: space-between;
  box-sizing: border-box;
} 
.loginBtn{
  position: absolute;
  bottom: 20%;
  left: 3%;
  width: 94%;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  color: #F5B32A;
  display: inline-block;
}
.userLogo{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}
.nickCenter{
  text-align: center;
  margin-top: 20%;
}
.nickNameC{
  margin-top: 10%;
  font-size: 14pt;
  font-family:PingFangSC-Semibold;
  font-weight:600;
  color:rgba(51,51,51,1);
  letter-spacing:1rpx;
}