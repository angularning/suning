/* components/personalList/personalList.wxss */

.list {
  display: flex;
  flex-flow: row;
  width: 100%;
  background: #fff;
  padding: 14rpx 2%;
}

.listImg {
}

.listImg image {
  width: 160rpx;
  height: 160rpx;
  line-height: 160rpx;
}

.iconCheck image {
  width: 120rpx;
  height: 54rpx;
  line-height: 54rpx;
}

.listNameAndTel {
  margin-left: 20rpx;
  width: 80%;
}

.listNameAndPlace {
  height: 80rpx;
  line-height: 90rpx;
}

.listTel {
  height: 80rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.listName {
  font-size: 32rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(51, 51, 51, 1);
}

.listPlace {
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  margin-left: 10rpx;
}

.iconCheck {
  margin-top: 7%;
}
